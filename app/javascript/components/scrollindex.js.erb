const scrollIndex = () => {

  $(document).ready(function () {
    const header = document.getElementById('page_header');
    if (header != null) {
      if (header.innerText == "All your animals") {
        $('html, body').animate({
            scrollTop: $('#page_header').offset().top
        }, 'slow');
      }
    }
  });
}

export { scrollIndex };
